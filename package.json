{
  "name": "fishingApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-5.0": "$ANDROID_HOME/tools/emulator @Nexus_4_API_21",
    "android-assemble-debug": "cd android && ./gradlew assembleDebug; cd -",
    "android-install-debug-apk": "android-uninstall; adb install android/app/build/outputs/apk/debug/app-debug.apk",
    "android-uninstall": "adb uninstall com.rn0605",
    "android-assemble-release": "cd android && ./gradlew assembleRelease; cd -",
    "codepush-deploy": "code-push deployment ls fishingAndroid; code-push deployment ls fishingiOS",
    "codepush:android:staging": "code-push release-react fishingAndroid android --dev false --targetBinaryVersion \"0.0.1\" --des \"code push for Android Staging\" -d Staging -m",
    "codepush:android:production": "code-push release-react fishingAndroid android --dev false --targetBinaryVersion \"0.0.1\" --des \"有紧急版本更新，请及时安装\" -d Production -m",
    "codepush:android:history": "code-push deployment history fishingAndroid Production",
    "codepush:android:updateInfo": "code-push deployment ls fishingAndroid",
    "fix-package": "node scripts/fix-package.ts development 0.0.1",
    "lint": "eslint .",
    "ios": "react-native run-ios",
    "postinstall": "yarn fix-package",
    "postuninstall": "yarn fix-package",
    "start": "react-native start",
    "start-debugger": "env REACT_DEBUGGER=\"$([ -d '/Applications/React Native Debugger.app' ] && echo \"open 'rndebugger://set-debugger-loc?host=localhost&port=8081' --args\")\" node node_modules/react-native/local-cli/cli.js start --port 8081",
    "switch-xcscheme-debug": "node scripts/xcscheme-switcher.ts weightapp-debug",
    "switch-xcscheme-release": "node scripts/xcscheme-switcher.ts weightapp-release",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.7",
    "@react-navigation/native": "^5.7.5",
    "@react-navigation/stack": "^5.9.2",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-amap3d": "^2.0.2",
    "react-native-background-timer": "^2.4.1",
    "react-native-code-push": "^6.3.0",
    "react-native-device-info": "^6.2.0",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "^1.13.1",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.14",
    "@types/react": "^16.9.51",
    "@types/react-native": "^0.63.25",
    "@types/react-test-renderer": "^16.9.3",
    "babel-jest": "^26.5.2",
    "eslint": "^7.11.0",
    "jest": "^26.5.2",
    "metro-react-native-babel-preset": "^0.63.0",
    "react-test-renderer": "16.13.1",
    "typescript": "^4.0.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
